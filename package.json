{
  "name": "contact-cards",
  "version": "1.0.0",
  "description": "module 19 project",
  "private": true,
  "scripts": {
    "start:dev": "concurrently \"cd server && npm run server\" \"cd client && npm run dev\"",
    "start": "cd server && node server.js",
    "server": "cd server node server.js --ignore client",
    "build": "cd client && npm run build",
    "install": "concurrently \"cd client && npm install\" \"cd server && npm install\" ",
    "client": "cd client && npm start",
    "heroku-prebuild": "npm install --dev"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^7.5.0"
  },
  "engines": {
    "node": "17.x"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LibhartLL/contact-card.git"
  },
  "bugs": {
    "url": "https://github.com/LibhartLL/contact-card/issues"
  },
  "homepage": "https://github.com/LibhartLL/contact-card#readme"
}
